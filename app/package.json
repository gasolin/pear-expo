{
  "name": "pear-expo",
  "version": "0.3.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "bundle": "./script/bundle_worklet.sh && ./script/install_addon.sh",
    "android": "npm run bundle && expo run:android",
    "ios": "npm run bundle && expo run:ios",
    "web": "expo start --web",
    "andlog": "adb logcat '*:S bare:*'",
    "ioslog": "log stream --predicate \"subsystem == 'bare'\" --level debug"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.1",
    "bare-rpc": "0.1.4",
    "events": "^3.3.0",
    "expo": "~52.0.32",
    "expo-build-properties": "^0.13.2",
    "expo-status-bar": "~2.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-bare-kit": "0.4.7",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.26.8"
  }
}
